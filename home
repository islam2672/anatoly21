                <section id="content2">
                    <div class="wrapper">
                        <!-- Tab one CARDIOLOGY -->
                        <div class="accord">
                            <input id="tab-one" class="tab-input" type="checkbox" name="tabs">
                            <label for="tab-one" class="tab-label">CARDIOLOGY</label>
                            <div class="accord-cont">

                                <!-- inside accord General Calculators -->
                                <div class="accord">
                                    <input id="tab-1" class="tab-input" type="checkbox" name="tabs">
                                    <label for="tab-1" class="tab-label">GENERAL CALCULATORS</label>
                                    <div class="accord-cont">
                                        <div class="alpha">
                                            <a href="https://docmode.org/ccs-saf-symptom-score/" target="_blank">
                                                <div class="hPAHEM">
                                                    <h4>CCS-SAF Symptom Score</h4>
                                                    <span class="gDoabK">Canadian Cardiovascular Society (CCS) Severity
                                                        of
                                                        Atrial
                                                        Fibrillation (SAF) scale describes severity of symptoms in
                                                        atrial
                                                        fibrillation.</span>
                                                </div>
                                            </a>
                                            <a href="https://docmode.org/ccs-saf-symptom-score/" target="_blank">

