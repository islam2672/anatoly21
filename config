            </div>
        </div>
        <!-- END: Order Summary -->

        <!-- START: Checkout Button -->
        <div class="fkcart-checkout-wrap fkcart-panel">
            <div class="fkcart-shimmer" style="width:100%;height:40px;margin:8px 0;"></div>

        </div>
        <!-- END: Checkout Button -->

    </div>
