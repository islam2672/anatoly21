### HOSTINGER LITESHIELD RECAPTCHA, DO NOT REMOVE THIS LINE ###
RewriteEngine On
LsRecaptcha 100
RewriteRule .* - [E=verifycaptcha:drop]
### HOSTINGER LITESHIELD RECAPTCHA, DO NOT REMOVE THIS LINE ###
# BEGIN WordPress
<IfModule mod_rewrite.c>
RewriteEngine On
RewriteBase /
RewriteRule ^index\.php$ - [L]
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule . /index.php [L]
</IfModule>
# END WordPress
